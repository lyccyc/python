def get_shrimp_num(heights: list):
    ans = 0
    sum = 0
    heights = [int(x) for x in input().split(" ")]
    heights = sorted(heights)
    # print(heights)
    a = len(heights)
    for i in range(a // 2):
        ans = abs(heights[i] - heights[len(heights) - 1 - i])
        sum += ans
    print(sum)


if __name__ == "__main__":
    heights = []
    get_shrimp_num(heights)
